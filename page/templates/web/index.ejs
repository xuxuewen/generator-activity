<% var page = { title:'请输入标题',keywords:'请输入关键字',description:'请输入标题'} %>
<%- include common/layout/header %>

<%# page content %>
<div class="page-wrap">
  <%#  这里开始填写内容 %>
  这是一个web端的页面
</div>

<%# loading %>
<div class="page-loading">
  <svg width="3rem"
    height="3rem"
    xmlns="http://www.w3.org/2000/svg"
    viewBox="0 0 100 100"
    preserveAspectRatio="xMidYMid"
    class="lds-ripple">
    <circle cx="50" cy="50" r="16.5429" fill="none" ng-attr-stroke="{{config.c1}}" ng-attr-stroke-width="{{config.width}}" stroke="#1d3f72" stroke-width="1">
      <animate attributeName="r" calcMode="spline" values="0;20" keyTimes="0;1" dur="2.8" keySplines="0 0.2 0.8 1" begin="-1.4s" repeatCount="indefinite"></animate>
      <animate attributeName="opacity" calcMode="spline" values="1;0" keyTimes="0;1" dur="2.8" keySplines="0.2 0 0.8 1" begin="-1.4s" repeatCount="indefinite"></animate>
    </circle>
    <circle cx="50" cy="50" r="6.68225" fill="none" ng-attr-stroke="{{config.c2}}" ng-attr-stroke-width="{{config.width}}" stroke="#5699d2" stroke-width="1">
      <animate attributeName="r" calcMode="spline" values="0;20" keyTimes="0;1" dur="2.8" keySplines="0 0.2 0.8 1" begin="0s" repeatCount="indefinite"></animate>
      <animate attributeName="opacity" calcMode="spline" values="1;0" keyTimes="0;1" dur="2.8" keySplines="0.2 0 0.8 1" begin="0s" repeatCount="indefinite"></animate>
    </circle>
  </svg>
</div>
<%# error %>
<div class="page-error">

</div>

<%- include common/layout/footer %>
